    "rollup-plugin-svelte": "^6.1.0",
    "rollup-plugin-typescript2": "^0.29.0",
    "svelte": "^3.29.4",
    "svelte-preprocess": "^4.5.2",
    "svelte-types-writer": "^1.0.1",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5"
